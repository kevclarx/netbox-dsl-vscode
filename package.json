{
  "name": "netbox-dsl-vscode",
  "displayName": "netbox-dsl-vscode",
  "description": "Netbox DSL vscode extension for defining infrastructure",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "netboxdsl.parse",
        "title": "Parse Netbox DSL",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "textMateLanguages": [
      {
        "scopeName": "source.netboxdsl",
        "path": "./syntaxes/netboxdsl.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "netboxdsl",
        "aliases": [
          "Netbox DSL"
        ],
        "extensions": [
          ".ndsl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "netboxTree",
          "name": "Netbox"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "netboxdsl.parse",
          "group": "navigation",
          "when": "view == netboxTree"
        }
      ]
    },
    "textDocumentContentProviders": [
      {
        "scheme": "netboxdsl",
        "language": "netboxdsl",
        "pattern": "**/*.ndsl"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}